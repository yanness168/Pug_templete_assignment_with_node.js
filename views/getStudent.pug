doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Document
    body 
     h2 Students
        for student in studentss
            ul(style={"font-weight":"bold"}) Name: !{JSON.stringify(student.name)}
                li(style={"font-weight":"normal"}) Student ID: !{JSON.stringify(student.id)} 
                li(style={"font-weight":"normal"}) Department: !{JSON.stringify(student.department)} 
                li(style={"font-weight":"normal"}) Semester: !{JSON.stringify(student.semester)}  
                li(style={"font-weight":"normal"}) Courses enrolled: !{JSON.stringify(student.courses_enrolled)}
                li(style={"font-weight":"normal"}) Courses completed: !{JSON.stringify(student.courses_completed)}  
            
             
